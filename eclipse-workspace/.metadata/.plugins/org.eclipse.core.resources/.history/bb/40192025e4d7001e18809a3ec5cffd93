package com.learning.core.day01;

public class Book {
	String bookTitle;
	double bookPrice;
	
	public Book(String bookTitle, double bookPrice)
	{
		this.bookTitle = bookTitle;
		this.bookPrice = bookPrice;
	}

	public void setBookTitle(String title)		//setter method for book title
	{
		bookTitle = title;
	}
	
	public String getBookTitle()				// getter method for book title
	{
		return bookTitle;
	}
	
	public void setBookPrice(double price)		//setter method for book price
	{
		bookPrice = price;
	}
	
	public String getBookPrice()				// getter method for book price
	{
		return bookPrice;
	}
}






